
+b-brands-of-goods()
  +b('section').brands-of-goods
    +b-headline('Бренды  c которыми работаем', 'Бренды товаров', 'Мы заботимся о красоте наших клиентов и передаем знания')
    +e.row
      each brand in brandsImages
        +e(a).item(href= brand.url)
          +e('img').img(src= brand.img, title= brand.title)
